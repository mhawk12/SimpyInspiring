{
  "manifest_version": 2,
  "name": "Simply Inspiring",
  "description": "A quote a day keeps the laziness away ",
  "version": "0.0.0.1",
  "author": "Nishant Shekhar",
  "browser_action": {
    "default_title": "Simply Inspiring"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "/icons/letter-s-16.png",
    "32": "/icons/letter-s-32.png",
    "128": "/icons/letter-s-128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}